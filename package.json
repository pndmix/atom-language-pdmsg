{
  "name": "language-pdmsg",
  "main": "./lib/index",
  "version": "0.4.1",
  "description": "language support for pdmsg",
  "keywords": [
    "language",
    "grammar",
    "pdmsg"
  ],
  "repository": "https://github.com/pndmix/atom-language-pdmsg",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "tree-sitter-pdmsg": "^0.1.2"
  },
  "devDependencies": {
    "@types/atom": "^1.40.5",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint src/**/*.ts && prettier -c **/*.{ts,less,json,yml}",
    "format": "eslint --fix src/**/*.ts && prettier --write **/*.{ts,less,json,yml}"
  }
}
